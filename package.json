  "devDependencies": {
    "@types/jest": "29.0.3",
    "jest": "29.0.3",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.8.3"
  },
