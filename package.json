{
  "name": "easy-memoize",
  "version": "1.0.8",
  "description": "Memoization made easy",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "coveralls": "jest --config jestconfig.json --coverage --coverageReporters=text-lcov | coveralls",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/R3DST0RM/easy-memoize.git"
  },
  "keywords": [
    "memo",
    "react",
    "memoize",
    "memoization",
    "typescript",
    "javascript",
    "redux"
  ],
  "author": "Dominik Schwarzbauer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/R3DST0RM/easy-memoize/issues"
  },
  "homepage": "https://github.com/R3DST0RM/easy-memoize#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "coveralls": "^3.1.0",
    "jest": "^27.3.1",
    "prettier": "^2.1.2",
    "ts-jest": "^27.0.6",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  }
}
